{
	"executor": "Docker",
	"image": "podman:test",
	"cpus": 2,
	"executable": "/workspace/snappy/build/snappy_uncompress_fuzzer",
	"args": ["-workers=2", "-close_fd_mask=3", "-reload=1", "-close_fd_mask=3", "corpus/"],
	"envs": {
		"HELLO": "WORLD"
	},
	"cwd": "/workspace/fuzzing",
	"corpus": {
		"path": "corpus",
		"label": "snappy_uncompress",
		"refresh_interval": 600,
		"upload": true,
		"upload_filter": ".*"
	}
}
